<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Label displayed while something is loading. -->
	<key>LOADING</key>
	<string>Lädt…</string>

	<!-- Button that chooses the selected app in the open panel. -->
	<key>CHOOSE</key>
	<string>Auswählen</string>

	<!-- Title of the open panel. -->
	<key>CHOOSE_APPLICATION</key>
	<string>Applikation auswählen</string>

	<!-- Title of the About window. -->
	<key>ABOUT</key>
	<string>Über</string>

	<!-- Message displayed when the app fails to be opened. -->
	<key>OPENING_APP_FAILED</key>
	<string>Die App konnte nicht gestartet werden, da ein unbekannter Fehler auftrat.</string>
</dict>
</plist>
