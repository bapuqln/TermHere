<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Label displayed while something is loading. -->
	<key>LOADING</key>
	<string>読み込み中・・・</string>
	
	<!-- Done button label. -->
  	<key>DONE</key>
  	<string>完了</string>

	<!-- Button that chooses the selected app in the open panel. -->
	<key>CHOOSE</key>
	<string>選択</string>

	<!-- Title of the open panel. -->
	<key>CHOOSE_APPLICATION</key>
	<string>アプリを選択</string>
	
	<!-- Title of the Help window. -->
  	<key>HELP</key>
  	<string>ヘルプ</string>

	<!-- Title of the About window. -->
	<key>ABOUT</key>
	<string>TermHereについて</string>

	<!-- Message displayed when the app fails to be opened. -->
	<key>OPENING_APP_FAILED</key>
	<string>不明なエラーがおこりましただからアプリを開くことできませんでした</string>
</dict>
</plist>
