<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Label displayed while something is loading. -->
	<key>LOADING</key>
	<string>加载中...</string>

	<!-- Button that chooses the selected app in the open panel. -->
	<key>CHOOSE</key>
	<string>选择</string>

	<!-- Title of the open panel. -->
	<key>CHOOSE_APPLICATION</key>
	<string>选择应用程序</string>

	<!-- Title of the About window. -->
	<key>ABOUT</key>
	<string>关于</string>

	<!-- Message displayed when the app fails to be opened. -->
	<key>OPENING_APP_FAILED</key>
	<string>由于未知原因无法启动.</string>
</dict>
</plist>
