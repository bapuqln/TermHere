<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Label displayed while something is loading. -->
	<key>LOADING</key>
	<string>Carregando...</string>

	<!-- Button that chooses the selected app in the open panel. -->
	<key>CHOOSE</key>
	<string>Escolhe</string>

	<!-- Title of the open panel. -->
	<key>CHOOSE_APPLICATION</key>
	<string>Escolha aplicativo</string>

	<!-- Title of the About window. -->
	<key>ABOUT</key>
	<string>Sobre</string>

	<!-- Message displayed when the app fails to be opened. -->
	<key>OPENING_APP_FAILED</key>
	<string>Não foi possível iniciar o aplicativo porque ocorreu um erro desconhecido.</string>
</dict>
</plist>
