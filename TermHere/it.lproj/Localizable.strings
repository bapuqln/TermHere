<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Label displayed while something is loading. -->
	<key>LOADING</key>
	<string>Caricamento...</string>

	<!-- Done button label. -->
  	<key>DONE</key>
  	<string>Fatto</string>

	<!-- Button that chooses the selected app in the open panel. -->
	<key>CHOOSE</key>
	<string>Scegli</string>

	<!-- Title of the open panel. -->
	<key>CHOOSE_APPLICATION</key>
	<string>Scegli applicazione</string>

	<!-- Title of the Help window. -->
  	<key>HELP</key>
  	<string>Aiuto</string>

	<!-- Title of the About window. -->
	<key>ABOUT</key>
	<string>Info</string>

	<!-- Message displayed when the app fails to be opened. -->
	<key>OPENING_APP_FAILED</key>
	<string>Non posso lanciare l'app a causa di un errore sconosciuto.</string>
</dict>
</plist>
