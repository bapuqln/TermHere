<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Label displayed while something is loading. -->
	<key>LOADING</key>
	<string>Chargement…</string>

	<!-- Done button label. -->
  	<key>DONE</key>
  	<string>Terminé</string>

	<!-- Button that chooses the selected app in the open panel. -->
	<key>CHOOSE</key>
	<string>Choisir</string>

	<!-- Title of the open panel. -->
	<key>CHOOSE_APPLICATION</key>
	<string>Choisir une application</string>

	<!-- Title of the Help window. -->
  	<key>HELP</key>
  	<string>Aide</string>

	<!-- Title of the About window. -->
	<key>ABOUT</key>
	<string>A propos</string>

	<!-- Message displayed when the app fails to be opened. -->
	<key>OPENING_APP_FAILED</key>
	<string>Impossible de lancer l'application une erreur inconnue s'est produite.</string>
</dict>
</plist>
