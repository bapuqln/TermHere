<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Label displayed while something is loading. -->
	<key>LOADING</key>
	<string>Загрузка...</string>

	<!-- Done button label. -->
	<key>DONE</key>
	<string>Готово</string>

	<!-- Button that chooses the selected app in the open panel. -->
	<key>CHOOSE</key>
	<string>Выбрать</string>

	<!-- Title of the open panel. -->
	<key>CHOOSE_APPLICATION</key>
	<string>Выбрать Приложение</string>

	<!-- Title of the Help window. -->
	<key>HELP</key>
	<string>Помощь</string>

	<!-- Title of the About window. -->
	<key>ABOUT</key>
	<string>О приложении</string>

	<!-- Message displayed when the app fails to be opened. -->
	<key>OPENING_APP_FAILED</key>
	<string>Не удалось запустить приложение: возникла неизвестная ошибка.</string>
</dict>
</plist>
