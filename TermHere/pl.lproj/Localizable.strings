<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Label displayed while something is loading. -->
	<key>LOADING</key>
	<string>Wczytywanie…</string>

	<!-- Done button label. -->
  	<key>DONE</key>
  	<string>Gotowe</string>

	<!-- Button that chooses the selected app in the open panel. -->
	<key>CHOOSE</key>
	<string>Wybierz</string>

	<!-- Title of the open panel. -->
	<key>CHOOSE_APPLICATION</key>
	<string>Wybierz aplikacje</string>

	<!-- Title of the Help window. -->
  	<key>HELP</key>
  	<string>Pomoc</string>

	<!-- Title of the About window. -->
	<key>ABOUT</key>
	<string>Informacje</string>

	<!-- Message displayed when the app fails to be opened. -->
	<key>OPENING_APP_FAILED</key>
	<string>Aplikacja nie została uruchomiona z powodu nieznanego błędu.</string>
</dict>
</plist>
