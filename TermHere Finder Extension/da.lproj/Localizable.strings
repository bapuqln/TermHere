<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Button that opens the file in an app. %@ is the app name. -->
	<key>OPEN_IN_APP</key>
	<string>Åbn i %@</string>

	<!-- Tooltip explaining what the Open in Terminal button does. -->
	<key>NEW_TERMINAL_HERE_TOOLTIP</key>
	<string>Åbn et terminalvindue i denne mappe</string>

	<!-- Button that edits the file in an app. %@ is the app name. -->
	<key>EDIT_IN_APP</key>
	<string>Rediger i %@</string>

	<!-- Tooltip explaining what the Open in Editor button does. -->
	<key>EDIT_IN_APP_TOOLTIP</key>
	<string>Åbn %@ til den nuværende fil eller mappe</string>

	<!-- Button that executes the file in an app. %@ is the app name. -->
	<key>EXECUTE_IN_APP</key>
	<string>Eksekver i %@</string>

	<!-- Tooltip explaining what the button does. -->
	<key>EXECUTE_IN_APP_TOOLTIP</key>
	<string>Åbn %@ og eksekver den nuværende fil</string>

	<key>NOT_EXECUTABLE_TITLE</key>
	<string>Kunne ikke åbne %@, fordi det ikke er en eksekverbar fil.</string>

	<key>NOT_EXECUTABLE_MESSAGE</key>
	<string>Marker filen som eksekverbar for at fortsætte.</string>

	<!-- Message displayed in a file open panel asking the user to give permission to read the folder. %@ is the folder path. -->
	<key>GRANT_SANDBOX_PERMISSION_MESSAGE</key>
	<string>TermHere har brug for tilladelse til at tilgå nogle filer. Tryk på åben for at give TermHere tilladelse til at tilgå %@.</string>
</dict>
</plist>
