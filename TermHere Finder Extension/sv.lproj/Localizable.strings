<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Button that opens the file in an app. %@ is the app name. -->
	<key>OPEN_IN_APP</key>
	<string>Öppna i %@</string>

	<!-- Tooltip explaining what the Open in Terminal button does. -->
	<key>NEW_TERMINAL_HERE_TOOLTIP</key>
	<string>Öppna en terminal i den här mappen</string>

	<!-- Button that edits the file in an app. %@ is the app name. -->
	<key>EDIT_IN_APP</key>
	<string>Redigera med %@</string>

	<!-- Tooltip explaining what the Open in Editor button does. -->
	<key>EDIT_IN_APP_TOOLTIP</key>
	<string>Öppna %@ till nuvarande fil eller katalog</string>

	<!-- Button that executes the file in an app. %@ is the app name. -->
	<key>EXECUTE_IN_APP</key>
	<string>Exekvera i %@</string>

	<!-- Tooltip explaining what the button does. -->
	<key>EXECUTE_IN_APP_TOOLTIP</key>
	<string>Öppna %@ och exekvera nuvarande fil</string>

	<key>NOT_EXECUTABLE_TITLE</key>
	<string>Kunde inte öppna %@ då den inte är en körbar fil.</string>

	<key>NOT_EXECUTABLE_MESSAGE</key>
	<string>Markera filen som körbar för att fortsätta.</string>

	<!-- Message displayed in a file open panel asking the user to give permission to read the folder. %@ is the folder path. -->
    <key>GRANT_SANDBOX_PERMISSION_MESSAGE</key>
    <string>TermHere behöver tillstånd för att komma åt vissa filer. Tryck på Öppna för att ge TermHere tillgång till %@.</string>
</dict>
</plist>
